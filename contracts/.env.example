# Environment Variables for Smart Contract Deployment

# Private key for deployment (NEVER commit the actual .env file)
PRIVATE_KEY=your_private_key_here

# Moonbeam RPC URLs
MOONBEAM_RPC_URL=https://rpc.api.moonbeam.network
MOONRIVER_RPC_URL=https://rpc.api.moonriver.moonbeam.network
MOONBASE_RPC_URL=https://rpc.api.moonbase.moonbeam.network

# Ethereum RPC URLs (optional, for testing)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-api-key

# API keys for contract verification
MOONSCAN_API_KEY=your_moonscan_api_key_here
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas Reporter (optional)
REPORT_GAS=false
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
